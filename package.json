{
  "name": "dynamic-forms-extension",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.15.7",
    "@cap-js/hana": "^2.1.1",
    "@sap-cloud-sdk/http-client": "^4.1.2",
    "@sap-cloud-sdk/resilience": "^4.1.2",
    "@sap/cds": "^9",
    "@sap/xssec": "^4.10.0",
    "exceljs": "^4.4.0",
    "express": "^4",
    "pdfkit": "^0.17.2",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "auth": {
        "kind": "xsuaa"
      },
      "[development]": {
        "db": {
          "kind": "sqlite",
          "impl": "@cap-js/sqlite",
          "credentials": {
            "url": "db.sqlite"
          }
        },
        "auth": {
          "kind": "xsuaa"
        }
      },
      "[hybrid]": {
        "db": {
          "kind": "hana",
          "impl": "@cap-js/hana",
          "deploy-format": "hdbtable"
        }
      },
      "[production]": {
        "db": {
          "kind": "hana",
          "impl": "@cap-js/hana",
          "deploy-format": "hdbtable"
        },
        "Mobile.Dynamic.Forms": {
          "kind": "rest",
          "model": "srv/external/dynamic_forms_ext",
          "impl": "srv/external/dynamic_forms_ext.js",
          "credentials": {
            "destination": "API_DYNAMIC_FORMS"
          }
        }
      },
      "Mobile.Dynamic.Forms": {
        "kind": "rest",
        "model": "srv/external/dynamic_forms_ext",
        "impl": "srv/external/dynamic_forms_ext.js",
        "credentials": {
          "destination": "API_DYNAMIC_FORMS"
        }
      }
    }
  },
  "private": true,
  "workspaces": [
    "app/*"
  ]
}
